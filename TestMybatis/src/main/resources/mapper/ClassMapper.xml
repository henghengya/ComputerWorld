<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="mapper.ClassMapper">
    <select id="getById" resultType="Clazz" parameterType="int">
        select * from test_mybatis.class where class.id = #{id};
    </select>
    <select id="listAll" resultType="Clazz">
        select * from test_mybatis.class ;
    </select>
</mapper>